<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
            <stroke android:width="@dimen/divider_height" android:color="@color/gray_d"/>
        </shape>
    </item>
    <item android:bottom="@dimen/divider_height">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>

    <item android:top="@dimen/standard_margin_default" android:bottom="@dimen/standard_margin_default">
        <shape android:shape="rectangle">
            <solid android:color="@color/gray_d"/>
        </shape>
    </item>
    <item android:left="@dimen/divider_height" android:bottom="@dimen/standard_margin_default">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
</layer-list>